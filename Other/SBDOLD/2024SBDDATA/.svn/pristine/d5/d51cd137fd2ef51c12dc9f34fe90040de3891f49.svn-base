<Window x:Class="NO6_FunctionTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NO6_FunctionTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid KeyDown="Grid_KeyDown">
        <TextBox x:Name="BarcodeInputTextBox" HorizontalAlignment="Left" Height="26" Margin="27,195,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="201"/>
        <TextBlock HorizontalAlignment="Left" Height="18" Margin="27,177,0,0" TextWrapping="Wrap" Text="Barcode or Qrcode" VerticalAlignment="Top" Width="114"/>
        <Button x:Name="Printer1" Content="熱感應紙" HorizontalAlignment="Left" Height="62" Margin="27,39,0,0" VerticalAlignment="Top" Width="139" Click="Printer1_Click"/>
        <Button x:Name="Printer2" Content="長條貼紙" HorizontalAlignment="Left" Height="62" Margin="189,39,0,0" VerticalAlignment="Top" Width="139" Click="Printer2_Click"/>
        <TextBlock x:Name="ComText" HorizontalAlignment="Left" Height="91" Margin="33,266,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="735"><Run Text="Com scan:"/><LineBreak/><Run/></TextBlock>
        <TextBlock x:Name="status" HorizontalAlignment="Left" Height="27" Margin="33,382,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="735"/>

    </Grid>
</Window>
